{{#each this}}
  <li class='country-list-item'>
    <p class='country__name' style="font-weight: 700; font-size: 24px; margin: 0">
        <img
        class='country-list__flag'
        src='{{flags.svg}}'
        alt='Flag of {{name.official}}'
        width='35px'
        height='22px'
        style="margin-right: 10px;",
      />{{name.official}}</p>
  </li>
{{/each}}